import base64

weight_data = []
for i in range(1, 6):
    filename = f'group1-shard{i}of5.bin'
    with open(filename, 'rb') as f:
        data = base64.b64encode(f.read()).decode('utf-8')
        weight_data.append(data)
        print(f"âœ… {filename}: {len(data)} chars")

# Print the JavaScript array
print("\n// PASTE THIS INTO YOUR HTML:")
print("const EMBEDDED_WEIGHTS = [")
for i, data in enumerate(weight_data):
    print(f'  "{data}"{"," if i < 4 else ""}')
print("];")
